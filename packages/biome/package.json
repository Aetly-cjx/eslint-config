		"test": "vitest",
		"test:watch": "vitest --watch"
	},
	"dependencies": {
		"@biomejs/biome": "^1.5.2",
		"@biomejs/wasm-nodejs": "^1.5.2",
		"synckit": "^0.8.5"
	},
	"devDependencies": {
		"eslint": "^8.54.0",
		"tsx": "^3.14.0",
		"unbuild": "^2.0.0",
		"vitest": "^1.0.4"
	}
