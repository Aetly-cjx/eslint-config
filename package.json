{
	"name": "@estjs/eslint-config",
	"version": "0.9.2-beta.8",
	"packageManager": "pnpm@7.33.7",
	"description": "",
	"type": "module",
	"keywords": [],
	"license": "MIT",
	"homepage": "https://github.com/estjs/eslint-config#readme",
	"bugs": "https://github.com/estjs/eslint-config/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/estjs/eslint-config.git"
	},
	"author": "jiangxd <jiangxd2016@gmail.com>",
	"funding": "https://github.com/sponsors/estjs",
	"files": [
		"dist"
	],
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"require": "./dist/index.cjs",
			"import": "./dist/index.js"
		}
	},
	"typesVersions": {
		"*": {
			"*": [
				"./dist/*",
				"./dist/index.d.ts"
			]
		}
	},
	"sideEffects": false,
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"dev": "tsup --watch",
		"build": "tsup",
		"lint": "pnpm run build &&eslint --cache --fix ./",
		"test": "vitest -r test",
		"play": "pnpm run -C playground dev",
		"play:build": "pnpm run -C playground build",
		"play:test": "pnpm run -C playground test -u",
		"play:preview": "pnpm run -C playground preview",
		"typecheck": "tsc  --noEmit",
		"coverage": "vitest run --coverage",
		"prepublishOnly": "pnpm run build",
		"release": "bumpp --commit --tag --push && pnpm publish --no-git-checks --access public"
	},
	"peerDependencies": {
		"eslint": ">=8.40.0"
	},
	"dependencies": {
		"@biomejs/biome": "^1.5.3",
		"@biomejs/wasm-nodejs": "^1.5.3",
		"@eslint-types/jsdoc": "46.8.2-1",
		"@eslint-types/typescript-eslint": "^6.21.0",
		"@typescript-eslint/eslint-plugin": "^7.0.2",
		"@typescript-eslint/parser": "^7.0.2",
		"@unocss/eslint-plugin": "^0.58.5",
		"eslint-plugin-biome": "^0.9.7",
		"eslint-plugin-eslint-comments": "^3.2.0",
		"eslint-plugin-i": "^2.29.1",
		"eslint-plugin-jsdoc": "^48.1.0",
		"eslint-plugin-jsonc": "^2.13.0",
		"eslint-plugin-markdown": "^3.0.1",
		"eslint-plugin-n": "^16.6.2",
		"eslint-plugin-no-only-tests": "^3.1.0",
		"eslint-plugin-react": "^7.33.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"eslint-plugin-react-refresh": "^0.4.5",
		"eslint-plugin-sort-keys": "^2.3.5",
		"eslint-plugin-svelte": "^2.35.1",
		"eslint-plugin-toml": "^0.9.2",
		"eslint-plugin-unicorn": "^51.0.1",
		"eslint-plugin-unused-imports": "^3.1.0",
		"eslint-plugin-vitest": "^0.3.22",
		"eslint-plugin-vue": "^9.21.1",
		"eslint-plugin-yml": "^1.12.2",
		"globals": "^14.0.0",
		"jsonc-eslint-parser": "^2.4.0",
		"local-pkg": "^0.5.0",
		"vue-eslint-parser": "^9.4.2",
		"yaml-eslint-parser": "^1.2.2"
	},
	"devDependencies": {
		"@types/node": "^20.11.19",
		"bumpp": "^9.3.0",
		"eslint": "^8.56.0",
		"pnpm": "^8.15.3",
		"tsup": "^8.0.2",
		"typescript": "^5.3.3"
	},
	"engines": {
		"node": ">=16.14.0"
	}
}
