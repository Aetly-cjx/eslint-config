{
  "name": "@estjs/eslint-config-monorepo",
  "version": "0.7.2-beta.4",
  "private": true,
  "author": "estjs",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "eslint . --ext .js,.ts,.json,.vue,.md --fix",
    "prerelease": "pnpm -r run build",
    "prettier": "prettier --write --ignore-unknown ./packages",
    "release": "pnpm -r run build && bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@estjs/eslint-config": "*",
    "@estjs/eslint-plugin": "workspace:*",
    "bumpp": "^9.1.0",
    "eslint": "^8.40.0",
    "prettier": "^2.8.8"
  }
}
