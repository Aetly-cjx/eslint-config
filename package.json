{
  "name": "@estjs/eslint-config-monorepo",
  "version": "0.6.0-beta.0",
  "private": true,
  "author": "estjs",
  "license": "MIT",
  "scripts": {
    "build": "pnpm -r build",
    "test": "pnpm -r test",
    "lint": "eslint . --ext .js,.ts,.json,.vue,.md --fix",
    "prerelease": "pnpm -r run build",
    "release": "pnpm -r run build && bumpp package.json packages/*/package.json --commit --push --tag && pnpm -r publish --access public"
  },
  "devDependencies": {
    "@estjs/eslint-config": "*",
    "@types/node": "^18.15.11",
    "bumpp": "^9.1.0",
    "eslint": "^8.37.0",
    "eslint-plugin-atomic": "link:./packages/eslint-plugin-atomic",
    "typescript": "^5.0.3",
    "unocss": "^0.50.6"
  }
}
